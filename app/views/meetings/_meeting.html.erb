<div class="col-xs-12 col-sm-6">
  <div class="meeting" meeting-id="<%= meeting.id %>">
    <div class="space">
      <div class="meeting_background">
        <%= image_tag meeting.image_url(:thumb).to_s, class:"img-responsive" %>
        <div class="meeting_contents">
          <div class="meeting_box">
            日時
            <hr />
            <div><%= meeting.holds_at %>
            </div>
          </div>
          <div class="meeting_box">
            主催者
            <hr />
            <div><%= meeting.user.display_name if meeting.user %></div>
          </div>
        </div>
      </div>
      <div class="meeting_title">
        <%= meeting.title %>
      </div>
    </div>
    <div class="white_hover">
    </div>
  </div>
</div>
